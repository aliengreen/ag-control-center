{
  "name": "ag-control-center",
  "productName": "ag-control-center",
  "version": "1.0.1",
  "description": "An Interactive Control Center of Alien Green IoT system. This is a admin panel for AG users and devices.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "deploy": "electron-builder build --mac --publish always"
  },
  "keywords": [
    "map",
    "admin",
    "control",
    "center",
    "panel",
    "aliengreen",
    "alien"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aliengreen/ag-control-center.git"
  },
  "author": {
    "name": "Lasha Dolidze",
    "email": "lasha@picktek.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "app/assets/icons/mac/icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ag-control-center"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./app/index.html",
                  "js": "./app/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^12.4.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babili-webpack-plugin": "^0.1.2",
    "bulma": "^0.8.0",
    "dejavu-sans-extra-light": "^1.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^4.2.0",
    "file-loader": "^4.2.0",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "fuse.js": "^3.4.5",
    "html-loader": "^0.5.5",
    "jsoneditor": "^7.2.1",
    "jw-paginate": "^1.0.4",
    "leaflet": "^1.5.1",
    "leaflet-draw": "^1.0.4",
    "leaflet-draw-locales": "^1.1.1",
    "lodash": "^4.17.15",
    "mapbox-gl": "^1.6.0",
    "mapbox-gl-leaflet": "0.0.11",
    "moment": "^2.24.0",
    "node-polyglot": "^2.4.0",
    "node-sass": "^4.13.0",
    "path": "^0.12.7",
    "raw-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "url-loader": "^2.2.0",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.45",
    "@electron-forge/maker-deb": "^6.0.0-beta.45",
    "@electron-forge/maker-rpm": "^6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.45",
    "@electron-forge/maker-zip": "^6.0.0-beta.45",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.45",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "css-loader": "^3.2.0",
    "electron": "6.0.11",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.1.0",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1"
  }
}
